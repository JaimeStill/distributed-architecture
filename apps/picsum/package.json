{
  "name": "picsum",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build-core": "ng build core",
    "deploy": "ng build --configuration development --output-path docs --base-href /picsum/ && copy docs\\index.html docs\\404.html",
    "start": "ng serve --configuration development",
    "watch": "ng build core --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.2",
    "@angular/cdk": "^14.0.2",
    "@angular/common": "^14.0.2",
    "@angular/compiler": "^14.0.2",
    "@angular/core": "^14.0.2",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^14.0.2",
    "@angular/material": "^14.0.2",
    "@angular/platform-browser": "^14.0.2",
    "@angular/platform-browser-dynamic": "^14.0.2",
    "@angular/router": "^14.0.2",
    "hammerjs": "^2.0.8",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "^14.0.2",
    "@angular/compiler-cli": "^14.0.2",
    "@angular/language-service": "^14.0.2",
    "@types/node": "^18.0.0",
    "ng-packagr": "^14.0.2",
    "typescript": "^4.7.3"
  },
  "overrides": {
    "@angular/flex-layout": {
      "@angular/cdk": "^14.0.0",
      "@angular/common": "^14.0.0",
      "@angular/core": "^14.0.0",
      "@angular/platform-browser": "^14.0.0"
    }
  }
}
